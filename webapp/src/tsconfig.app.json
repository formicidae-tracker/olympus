{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"outDir": "../out-tsc/app",
		"types": [],
		"paths": {
			"@models/*":["src/app/core/models/*"],
			"@services/*":["src/app/core/services/*"],
			"@environments/*":["src/environments/*"]
		}
	},
	"files": [
		"main.ts",
		"polyfills.ts"
	],
	"include": [
		"src/**/*.d.ts"
	],
	"exclude": [
		"**/.#*"
	]
}
