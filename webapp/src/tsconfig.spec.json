{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"outDir": "../out-tsc/spec",
		"types": [
			"jasmine",
			"node"
		],
		"paths": {
			"@models/*":["src/app/core/models/*"],
			"@services/*":["src/app/core/services/*"],
			"@environments/*":["src/environments/*"]
		}
	},
	"files": [
		"test.ts",
		"polyfills.ts"
	],
	"include": [
		"**/*.spec.ts",
		"**/*.d.ts"
	],
	"exclude": [
		"**/.#*"
	]
}
